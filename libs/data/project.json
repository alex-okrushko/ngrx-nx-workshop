    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/data/src/**/*.ts",
          "libs/data/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
